    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "error-overlay-webpack-plugin": "^0.2.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.1",
